# Copy this file to .env and fill in your actual values

# ASI1 API Key (get from https://asi1.ai/)
ASI1_API_KEY=YOUR_ASI1_API_KEY

# Optional: Model name (default: asi1-mini)
ASI1_MODEL=asi1-mini

# Optional: Agent seed and name
AGENT_SEED=asi1-test-agent
AGENT_NAME=ASI1 Agent

# Optional: Max tokens for completion
MAX_TOKENS=1024

# Optional: Comma-separated list of addresses to bypass rate limit
BYPASS_RATE_LIMIT=
